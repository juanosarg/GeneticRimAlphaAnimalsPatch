<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Eldritch Incubators -->



	<RecipeDef>
		<defName>GR_CreateDinoBearIncubator</defName>
		<label>dinobear incubator</label>
		<description>Fill an incubator with genes from a bear and a dinosaur</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>

			<li>GR_ArcheoGenePod</li>				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_DinoGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>GR_EmptyIncubator</li>
				<li>GR_BearGenetic</li>
				<li>GR_DinoGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_DinoBearIncubator>1</GR_DinoBearIncubator>
		</products>
		<researchPrerequisite>GR_ArcheoGeneticEngineering</researchPrerequisite>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>

