<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Eldritch Incubators -->



	<RecipeDef>
		<defName>GR_CreateBearBugIncubator</defName>
		<label>bear bug incubator</label>
		<description>Fill an incubator with genes from a bear and a bed bug</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>

			<li>GR_AlphaGenePod</li>				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_AlphaGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>GR_EmptyIncubator</li>
				<li>GR_BearGenetic</li>
				<li>GR_AlphaGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_BearBugIncubator>1</GR_BearBugIncubator>
		</products>
		<researchPrerequisite>GR_AlphaAnimalGeneticEngineering</researchPrerequisite>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>

